     p_codes :
      -
        internal_properties : CAAAAAgAAAAZMNjVrYcgSaPomaMMQG61hmDSCFMXXAUBrpy+ul3jIZSBkeEg702Tkv3y39GYrUznu+2L6Fmm21iVlaIzSzMMX0Mjhwadu817ZDw9tzHA1/Wazz03vaNGL+A5x+25UPrjD3CaRG0NsFwWTlXwFRuXrm1neXOer6eKx/kMLZuWlQ==
        code : |1-
         
         sFichier 	est une chaine
         bufContenu	est un chaîne
         
         selon ONG_Onglet1
         	
         	// SQL
         	cas 1
         		
         		// Ouvre le sélecteur de fichiers
         		sFichier = fSélecteur("", "", "Sélectionnez un fichier...", "Code SQL (*.sql)"+TAB+"*.SQL"+RC+"Tous fichiers (*.*)"+TAB+"*.*", "SQL", fSelOuvre+fSelExiste)
         		
         	// XML
         	cas 2
         		
         		// Ouvre le sélecteur de fichiers
         		sFichier = fSélecteur("", "", "Sélectionnez un fichier...", "Documents XML (*.xml)"+TAB+"*.XML"+RC+"Tous fichiers (*.*)"+TAB+"*.*", "XML", fSelOuvre+fSelExiste)
         		
         FIN
         
         si sFichier <> "" alors
         	
         	sablier(vrai)
         	
         	// Charge le contenu du fichier
         	bufContenu = fChargeTexte(sFichier)
         	
         	selon ONG_Onglet1
         		
         		// SQL
         		cas 1
         			
         			// Charge le code
         			SQL:SetCode(bufContenu)
         			
         			// Effectue la coloration
         			SAI_CODESQL = sql:Coloration()
         			
         		// XML
         		cas 2
         						
         			// Charge le code
         			xml:SetCode(bufContenu)
         			
         			// Effectue la coloration
         			SAI_CODEXML = XML:Coloration()			
         	FIN
         	
         	Sablier(faux)
         FIN
