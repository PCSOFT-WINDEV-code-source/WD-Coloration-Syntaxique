     code : |1-
      
      // Colorisateur SQL
      SQL est un cColorationSQL()
      
      // Colorisation XML
      XML est un cColorationXML()
      
      // Colorisation HTML
      HTML est un cColorationHTML()
      
      // Définit le mode de sortie pour les colorisateurs : mode RTF (dans un champ de saisie RTF)
      SQL:SetModeSortie(cColorationSQL::OUT_RTF)
      XML:SetModeSortie(cColorationSQL::OUT_RTF)
      HTML:SetModeSortie(cColorationSQL::OUT_RTF)
   -
     code : |1-
      
      // Consultez le code 'A chaque modification' des champs de saisie SAI_CODESQL, SAI_CODEXML et SAI_CODEHTML
      
      // Définit le code SQL/XML par défaut
      SQL:SetCode(rtfverstexte(SAI_CODESQL..valeurinitiale))
      xml:SetCode(RTFVersTexte(SAI_CODEXML..valeurinitiale))
      HTML:SetCode(RTFVersTexte(SAI_CODEHTML..valeurinitiale))
      
      // Effectue la coloration
      SAI_CODESQL = SQL:Coloration()
      SAI_CODEXML = XML:Coloration()
      SAI_CODEHTML = HTML:Coloration()
